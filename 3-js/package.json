{
  "name": "3-js",
  "version": "1.0.0",
  "description": "Teste front-end Pleno Prodigious BR",
  "author": "Prodigious BR",
  "license": "ISC",
  "main": "webpack.config.js",
  "scripts": {
    "install": "npm i",
    "startDev": "npm run dev && npm run openServerDev",
    "startProd": "npm run prod && npm run openServerProd",
    "cleanDevFolder": "rimraf ./dev/*",
    "cleanProdFolder": "rimraf ./prod/*",
    "removeNode": "rm -rf node_modules && npm cache clean -f",
    "uninstall": "npm run cleanDevFolder && npm run cleanProdFolder && npm run removeNode && rm package-lock.json",
    "reinstall": "npm run uninstall && npm run install",
    "dev": "npm run cleanDevFolder && npm run buildDev",
    "prod": "npm run cleanProdFolder && npm run buildProd",
    "buildDev": "webpack --node-env=development",
    "buildProd": "webpack --node-env=production",
    "openServerDev": "webpack-dev-server --node-env=development --open",
    "openServerProd": "webpack-dev-server --node-env=production --open"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.5.1",
    "cssnano": "^5.0.14",
    "cssnano-preset-advanced": "^5.1.9",
    "file-loader": "^6.2.0",
    "glob": "^7.2.0",
    "html-loader": "^3.0.1",
    "html-webpack-plugin": "^5.5.0",
    "liquidjs": "^9.31.0",
    "liquidjs-loader": "^1.0.1",
    "mini-css-extract-plugin": "^2.4.5",
    "mini-svg-data-uri": "^1.4.3",
    "path": "^0.12.7",
    "postcss-loader": "^6.2.1",
    "purgecss": "^4.1.3",
    "rimraf": "^3.0.2",
    "style-loader": "^3.3.1",
    "stylus": "^0.56.0",
    "stylus-loader": "^6.2.0",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0"
  }
}
